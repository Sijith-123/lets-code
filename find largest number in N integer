#include<stdio.h>
#include <stdlib.h>

int main()
{
    int a, b[100], temp;
    scanf("%d",&a);
    for(int i=0; i<a; i++)
    {
        scanf("%d",&b[i]);
    }
    for(int i=0; i<a; i++)
    {
        for(int j=i+1; j<a; j++)
        {
            if(b[i]<b[j])
            {
                temp=b[i];
                b[i]=b[j];
                b[j]=temp;
            }
        }
    }
    printf("%d",b[1]);
}
